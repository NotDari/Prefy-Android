<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/popularPageHostFragment">
    <fragment
        android:id="@+id/Profile_fragment"
        android:name="com.example.prefy.Profile.HomeProfileFragment"
        tools:layout="@layout/fragment_profile" >
        <action
            android:id="@+id/action_Profile_fragment_to_profilePostListFragment"
            app:destination="@id/profilePostListFragment" />
    </fragment>
    <fragment
        android:id="@+id/profilePostListFragment"
        android:name="com.example.prefy.Profile.ProfileListPostRec.ProfilePostListFragment"

        tools:layout="@layout/fragment_profile_post_list" >
        <argument
            android:name="positionClicked"
            app:argType="integer"
            android:defaultValue="0" />
        <argument
            android:name="postList"
            app:argType="com.example.prefy.customClasses.StandardPost[]" />
        <argument
            android:name="username"
            app:argType="string"
            android:defaultValue="none" />
        <argument
            android:name="profileImageLink"
            app:argType="string"
            android:defaultValue="none" />
    </fragment>
    <fragment
        android:id="@+id/settingsFragment"
        android:name="com.example.prefy.Settings.SettingsFragment"
        android:label="fragment_settings"
        tools:layout="@layout/fragment_settings" >
        <action
            android:id="@+id/action_settingsFragment_to_preferencesFragment"
            app:destination="@id/preferencesFragment" />
        <action
            android:id="@+id/action_settingsFragment_to_feedbackFragment"
            app:destination="@id/feedbackFragment" />
        <action
            android:id="@+id/action_settingsFragment_to_settingsOtherFragment"
            app:destination="@id/settingsOtherFragment" />
        <action
            android:id="@+id/action_settingsFragment_to_detailsEditGatewayFragment"
            app:destination="@id/detailsEditGatewayFragment" />
    </fragment>
    <action
        android:id="@+id/action_global_settingsFragment"
        app:destination="@id/settingsFragment"
        app:enterAnim="@anim/bottom_to_top"
        app:popExitAnim="@anim/top_to_bottom" />
    <fragment
        android:id="@+id/commentsFragment"
        android:name="com.example.prefy.Comments.CommentsFragment"
        android:label="fragment_comments"
        tools:layout="@layout/fragment_comments" >
        <argument
            android:name="post"
            app:argType="com.example.prefy.customClasses.StandardPost" />
        <argument
            android:name="user"
            app:argType="com.example.prefy.Profile.User" />
    </fragment>
    <action android:id="@+id/action_global_commentsFragment" app:destination="@id/commentsFragment" />
    <fragment
        android:id="@+id/userProfile"
        android:name="com.example.prefy.Profile.OtherUsers.UserProfileFragment"
        android:label="UserProfile" >
        <action
            android:id="@+id/action_userProfile_to_profilePostListFragment"
            app:destination="@id/profilePostListFragment" />
        <argument
            android:name="user"
            app:argType="com.example.prefy.Profile.User"
            app:nullable="true"
            android:defaultValue="@null" />
    </fragment>
    <action android:id="@+id/action_global_userProfile" app:destination="@id/userProfile" />
    <fragment
        android:id="@+id/popularPageHostFragment"
        android:name="com.example.prefy.Popular.PopularPageHostFragment"
        android:label="fragment_popular_page_host"
        tools:layout="@layout/fragment_popular_page_host" />
    <fragment
        android:id="@+id/activityFragment"
        android:name="com.example.prefy.Activity.ActivityHostFragment"
        android:label="fragment_activity"
        tools:layout="@layout/fragment_activity" />
    <dialog
        android:id="@+id/submitPostDialog"
        android:name="com.example.prefy.SubmitPost.SubmitPostDialog"
        tools:layout="@layout/post_bottom_dialog"
        />
    <fragment
        android:id="@+id/exploreHostFragment"
        android:name="com.example.prefy.Explore.ExploreHostFragment"
        android:label="fragment_explore_host"
        tools:layout="@layout/fragment_explore_host" >
        <action
            android:id="@+id/action_exploreHostFragment_to_exploreCategoriesPostFragment"
            app:destination="@id/exploreCategoriesPostFragment" />
        <action
            android:id="@+id/action_exploreHostFragment_to_exploreCategorySelectorFragment"
            app:destination="@id/exploreCategorySelectorFragment" />
    </fragment>
    <fragment
        android:id="@+id/preferencesFragment"
        android:name="com.example.prefy.Settings.Preferences.PreferencesFragment"
        android:label="fragment_preferences"
        tools:layout="@layout/fragment_preferences" />
    <fragment
        android:id="@+id/feedbackFragment"
        android:name="com.example.prefy.Settings.Feedback.FeedbackFragment"
        android:label="fragment_feedback"
        tools:layout="@layout/fragment_feedback" />
    <fragment
        android:id="@+id/settingsOtherFragment"
        android:name="com.example.prefy.Settings.Other.SettingsOtherFragment"
        android:label="fragment_settings_other"
        tools:layout="@layout/fragment_settings_other" />
    <fragment
        android:id="@+id/searchFragment"
        android:name="com.example.prefy.Search.SearchFragment"
        android:label="fragment_search"
        tools:layout="@layout/fragment_search" /><action android:id="@+id/action_global_searchFragment" app:destination="@id/searchFragment"/>
    <fragment
        android:id="@+id/exploreCategoriesPostFragment"
        android:name="com.example.prefy.Explore.ExploreCategories.ExploreCategoriesPosts.ExploreCategoriesPostFragment"
        tools:layout="@layout/fragment_explore_categories_post" >
        <argument
            android:name="category"
            app:argType="string"
            android:defaultValue="none" />
    </fragment>
    <fragment
        android:id="@+id/exploreCategorySelectorFragment"
        android:name="com.example.prefy.Explore.ExploreCategories.ExploreCategoriesSelector.ExploreCategorySelectorFragment"
        android:label="fragment_explore_category_selector"
        tools:layout="@layout/fragment_explore_category_selector" >
        <argument
            android:name="categoryImageSize"
            app:argType="integer"
            android:defaultValue="0" />
        <action
            android:id="@+id/action_exploreCategorySelectorFragment_to_exploreCategoriesPostFragment"
            app:destination="@id/exploreCategoriesPostFragment" />
    </fragment>
    <fragment
        android:id="@+id/reportFragment"
        android:name="com.example.prefy.Report.ReportFragment"
        android:label="fragment_report"
        tools:layout="@layout/fragment_report" >
        <argument
            android:name="post"
            app:argType="com.example.prefy.customClasses.StandardPost"
            app:nullable="true"
            android:defaultValue="@null" />
        <argument
            android:name="Type"
            app:argType="string"
            android:defaultValue="none" />
        <argument
            android:name="comment"
            app:argType="com.example.prefy.Comments.Comment"
            app:nullable="true"
            android:defaultValue="@null" />
    </fragment><action android:id="@+id/action_global_reportFragment" app:destination="@id/reportFragment"/>
    <fragment
        android:id="@+id/detailsEditGatewayFragment"
        android:name="com.example.prefy.Settings.DetailsEdit.DetailsEditGatewayFragment"
        android:label="fragment_details_edit_gateway"
        tools:layout="@layout/fragment_details_edit_gateway" >
        <action
            android:id="@+id/action_detailsEditGatewayFragment_to_detailsEditFragment"
            app:destination="@id/detailsEditFragment" />
        <argument
            android:name="detail"
            app:argType="string"
            app:nullable="true" />
    </fragment>
    <fragment
        android:id="@+id/detailsEditFragment"
        android:name="com.example.prefy.Settings.DetailsEdit.DetailsEditFragment"
        android:label="fragment_details_edit"
        tools:layout="@layout/fragment_details_edit" >
        <argument
            android:name="detail"
            app:argType="string"
            app:nullable="true" />
    </fragment>
</navigation>